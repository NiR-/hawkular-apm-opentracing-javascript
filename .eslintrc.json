{
    "parser": "babel-eslint",
    "plugins": [
        "babel"
    ],
    "extends": "eslint-config-airbnb",
    "rules": {
       "indent": ["error", 4],
       "no-underscore-dangle": [0],
       "no-param-reassign": [0],
       "class-methods-use-this": [0],
       "max-len": ["error", 130],
       "global-require": [0],
       "no-empty": [0]
   }
}
